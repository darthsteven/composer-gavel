{
    "name": "deviantintegral/composer-gavel",
    "description": "Enforce a minimum Composer version for your team",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Berry",
            "email": "deviantintegral@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "composer-plugin-api": "^1.1 || ^2.0"
    },
    "require-dev": {
        "composer/composer": "^1.6",
        "phpunit/phpunit": "5 - 7",
        "mikey179/vfsstream": "^1.6"
    },
    "autoload": {
        "psr-4": {"Deviantintegral\\ComposerGavel\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"Deviantintegral\\ComposerGavel\\Tests\\": "tests"}
    },
    "extra": {
        "class": "Deviantintegral\\ComposerGavel\\ComposerVersionRequirement"
    }
}
